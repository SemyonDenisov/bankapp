{
    "id": null,
    "title": "Authentication Metrics",
    "tags": ["spring", "security"],
    "panels": [
      {
        "type": "graph",
        "title": "Successful Logins",
        "targets": [
          {
            "expr": "auth_login_success_total",
            "legendFormat": "{{username}}",
            "refId": "A"
          }
        ]
      },
      {
        "type": "graph",
        "title": "Failed Logins",
        "targets": [
          {
            "expr": "auth_login_fail_total",
            "legendFormat": "{{username}}",
            "refId": "B"
          }
        ]
      }
    ]
  }
  